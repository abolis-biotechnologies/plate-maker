{
  "$schema": "../../../node_modules/ng-packagr/package.schema.json",
  "name": "@abolis/plate-maker",
  "version": "1.0.2",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/abolis-biotechnologies/plate-maker"
  },
  "description": "Open source Front-End tool to simulate and visualize a microplate",
  "keywords": [
    "frontend",
    "plate",
    "biology"
  ],
  "homepage": "https://github.com/abolis-biotechnologies/plate-maker#readme",
  "bugs": {
    "url": "https://github.com/abolis-biotechnologies/plate-maker/issues"
  },
  "ngPackage": {
    "dest": "../../../dist/plate-maker",
    "lib": {
      "entryFile": "src/public_api.ts",
      "umdModuleIds": {
        "ngx-drag-to-select": "ngx-drag-to-select",
        "ngx-popper": "ngx-popper",
        "angular-animations": "angular-animations",
        "string-algorithms": "string-algorithms"
      }
    }
  },
  "peerDependencies": {
    "@angular/common": "^9.0.1",
    "@angular/core": "^9.0.1",
    "mdbootstrap": "~4.10.0",
    "ngx-drag-to-select": "^3.1.1",
    "ngx-popper": "^7.0.0",
    "popper.js": "^1.16.1",
    "angular-animations": "^0.0.10",
    "string-algorithms": "1.0.29"
  }
}
